package com.exam.dto;

public class AutomaticPaper {
	private Integer subject;
	
	private int choiceQuestions;
	
	private int gapFillings;
	
	private int checkQuestions;
	
	private int multipleChoiceQuestions;
	
	private int comprehensiveQuestions;

	public Integer getSubject() {
		return subject;
	}

	public void setSubject(Integer subject) {
		this.subject = subject;
	}

	public int getChoiceQuestions() {
		return choiceQuestions;
	}

	public void setChoiceQuestions(int choiceQuestions) {
		this.choiceQuestions = choiceQuestions;
	}

	public int getGapFillings() {
		return gapFillings;
	}

	public void setGapFillings(int gapFillings) {
		this.gapFillings = gapFillings;
	}

	public int getCheckQuestions() {
		return checkQuestions;
	}

	public void setCheckQuestions(int checkQuestions) {
		this.checkQuestions = checkQuestions;
	}

	public int getMultipleChoiceQuestions() {
		return multipleChoiceQuestions;
	}

	public void setMultipleChoiceQuestions(int multipleChoiceQuestions) {
		this.multipleChoiceQuestions = multipleChoiceQuestions;
	}

	public int getComprehensiveQuestions() {
		return comprehensiveQuestions;
	}

	public void setComprehensiveQuestions(int comprehensiveQuestions) {
		this.comprehensiveQuestions = comprehensiveQuestions;
	}

	@Override
	public String toString() {
		return "AutomaticPaper [subject=" + subject + ", choiceQuestions=" + choiceQuestions + ", gapFillings="
				+ gapFillings + ", checkQuestions=" + checkQuestions + ", multipleChoiceQuestions="
				+ multipleChoiceQuestions + ", comprehensiveQuestions=" + comprehensiveQuestions + "]";
	}
	
	

}
